@model IEnumerable<R_Mall.Models.Movy>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_theme.cshtml";
}

<h2>Multiplex</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.M_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.M_Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.M_Time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.M_Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.M_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.M_Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.M_Time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.M_Status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.M_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.M_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.M_Id })
        </td>
    </tr>
}

</table>
