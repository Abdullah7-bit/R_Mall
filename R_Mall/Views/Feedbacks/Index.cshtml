@model IEnumerable<R_Mall.Models.Feedback>

@{
    ViewBag.Title = "Index";

}

<h2>Feedback</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.User_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Message)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.User_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Message)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Fdbk_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Fdbk_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Fdbk_Id })
        </td>
    </tr>
}

</table>
